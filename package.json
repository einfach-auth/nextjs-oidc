  },
  "peerDependencies": {
    "next": "^15",
    "react": "^19"
  },
  "devDependencies": {
    "@types/node": "^22.13.2",
    "@types/react": "^19.0.8",
    "next": "^15.2.4",
    "prettier": "4.0.0-alpha.10",
    "react": "^19.0.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
